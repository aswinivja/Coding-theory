    "def xor_encrypt(data, key=\"1010\"):\n",
    "    key = list(map(int, key))\n",
    "    return [(int(bit) ^ key[i % len(key)]) for i, bit in enumerate(data)]\n",
    "\n",
    "def xor_decrypt(data, key=\"1010\"):\n",
    "    return xor_encrypt(data, key)  # same as encryption\n"
   ]
